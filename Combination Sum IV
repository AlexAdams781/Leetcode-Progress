# https://leetcode.com/problems/combination-sum-iv/
'''
Use DP to store subproblems for t in range 1 to target
'''

class Solution:
    def combinationSum4(self, nums: List[int], target: int) -> int:
        DP = [1] * (target+1)
        S = set(nums)
        for t in range(1, target+1):
            total = 0
            for s in S:
                if t-s >= 0:
                    total += DP[t-s]
            DP[t] = total

        return DP[target]
